<!-- SVG para crear favicon - luego convertiremos a ICO -->
<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
    <!-- Fondo negro (cubierta del cable) -->
    <circle cx="16" cy="16" r="15" fill="#000000"/>
    
    <!-- Centro colorido (fibras ópticas) -->
    <g transform="translate(16,16)">
        <!-- Fibras principales en colores vibrantes -->
        <circle cx="-4" cy="-3" r="2" fill="#FF0000"/>
        <circle cx="4" cy="-3" r="2" fill="#0000FF"/>
        <circle cx="-4" cy="3" r="2" fill="#00FF00"/>
        <circle cx="4" cy="3" r="2" fill="#FFA500"/>
        
        <!-- Fibras adicionales para mayor detalle -->
        <circle cx="-1" cy="-6" r="1.5" fill="#FFFF00"/>
        <circle cx="6" cy="0" r="1.5" fill="#FF00FF"/>
        <circle cx="-6" cy="0" r="1.5" fill="#00FFFF"/>
        <circle cx="0" cy="6" r="1.5" fill="#FF8000"/>
        
        <!-- Centro de conexión -->
        <circle cx="0" cy="0" r="1" fill="#FFFFFF"/>
    </g>
</svg>